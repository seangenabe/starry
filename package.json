{
  "name": "starry-main",
  "version": "1.3.0",
  "private": true,
  "description": "Modular functions for iterable objects",
  "homepage": "https://github.com/seangenabe/starry",
  "author": "Sean Genabe <seangenabe@outlook.com>",
  "repository": "seangenabe/starry",
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "main": "lib/starry.js",
  "keywords": [
    "iterable",
    "iterator",
    "iteration"
  ],
  "scripts": {
    "ava": "ava",
    "build": "npm run setup && npm run lerna-bootstrap && npm run tsc",
    "lerna-bootstrap": "lerna bootstrap --hoist typescript --hoist ava",
    "presetup": "cd build/setup && tsc",
    "setup": "node build/setup",
    "postsetup": "cp ./readme.md ./packages/starry/",
    "pretest": "npm run setup && npm run tsc",
    "nyc-ava": "nyc ava",
    "test": "nyc ava",
    "report": "nyc report --reporter=html",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "pub": "npm run build && lerna publish",
    "lint": "eslint .",
    "tsc": "lerna run tsc"
  },
  "devDependencies": {
    "@types/node": "^8.0.10",
    "ava": "^0.22.0",
    "chalk": "^2.0.1",
    "coveralls": "^3.0.0",
    "encody": "^1.0.0",
    "eslint": "^4.1.0",
    "eslint-config-mirage": "^2.1.0",
    "lerna": "^2.2.0",
    "lodash.assign": "^4.2.0",
    "lodash.camelcase": "latest",
    "lodash.includes": "^4.1.2",
    "lodash.merge": "^4.6.0",
    "loud-rejection": "^1.6.0",
    "mz": "^2.6.0",
    "nyc": "^11.0.1",
    "sort-package-json": "^1.4.0",
    "typescript": "^2.4.1"
  },
  "ava": {
    "files": [
      "packages/*/test.js"
    ]
  },
  "nyc": {
    "exclude": [
      "**/node_modules/",
      "packages/*/test.js"
    ]
  }
}
